{
  "name": "@okeeffed/postman-tools",
  "description": "Helper tools to generate Postman collections and environments",
  "version": "0.0.10",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Dennis O'Keeffe",
  "license": "CC-BY-NC-4.0",
  "files": [
    "dist/*.js",
    "dist/*.mjs",
    "dist/**/*.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "postman-tools": "dist/main.js"
  },
  "scripts": {
    "dev": "npx tsx src/main.ts",
    "build": "tsup",
    "build:publish": "npm run build && npm publish --access public",
    "test": "jest",
    "seed": "tsx seed.ts",
    "bootstrap": "bin/bootstrap.sh",
    "postman-schema": "tsx bin/postman-json-schema.ts"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/openapi-to-postmanv2": "^3.2.4",
    "@types/prettier": "^2.7.3",
    "@types/prompts": "^2.4.9",
    "@types/uuid": "^10.0.0",
    "cosmiconfig-typescript-loader": "^5.0.0",
    "jest": "29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^12.0.1",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "cosmiconfig": "^9.0.0",
    "es-toolkit": "^1.13.1",
    "json-refs": "^3.0.15",
    "json-schema-to-zod": "^2.4.0",
    "openapi-to-postmanv2": "^4.23.1",
    "prompts": "^2.4.2",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  }
}
